Project matcha {
  database_type: 'PostgreSQL'
  Note: 'matcha'
}

Table users {
  id integer [primary key, unique]
  username varchar [unique, not null]
  biography varchar
  gender varchar(1) [not null]
  created_at timestamp
}

Table sexual_preference {
  id integer [primary key, unique]
  sexual_preference varchar
}

Table interest {
  id integer [primary key, unique]
  interest varchar
}

Table picture {
  id integer [primary key, unique]
  is_profile_picture boolean [default false]
  picture_path varchar
}

Table user_interest {
  id_user integer [fk]
  id_interest integer [fk]
}

Table user_picture {
  id_user integer [fk]
  id_picture integer [fk]
}

Table user_sexual_preference {
  id_user integer [fk]
  id_sexual_preference integer [fk]
}
